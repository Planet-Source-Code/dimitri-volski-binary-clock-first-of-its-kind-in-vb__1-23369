Private Sub Command2_Click()
Timer1.Enabled = False
End Sub



Private Sub Form_Load()
Set picObjects(0) = LoadPicture("1.bmp")
Set picObjects(1) = LoadPicture("2.bmp")
End Sub

Private Sub Timer1_Timer()
ProceSSing
End Sub
