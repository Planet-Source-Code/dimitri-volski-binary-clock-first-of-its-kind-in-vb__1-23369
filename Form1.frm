Option Explicit
Dim picObjects(0 To 8) As Picture

Public Function WorkingProg(temp As Variant, Location As Integer)
Dim j As Integer

For j = 1 To 4
If Mid(temp, j, 1) = 1 Then
Picture1(j + ((Location - 1) * 4)).Picture = picObjects(1)
Else
Picture1(j + ((Location - 1) * 4)).Picture = picObjects(0)
End If


Next j

End Function


Private Sub ProceSSing()
Dim I As Integer
Dim temp As Integer
Dim num As Variant
Dim results
L2.Caption = Format$(Now, "hhmmss")


For I = 1 To 6
num = ""
temp = Mid(Format$(Now, "hhmmss"), I, 1)

Do
If temp Mod 2 = 1 Then
num = 1 & num
temp = (temp - 1) / 2
Else
num = 0 & num
temp = temp / 2
End If
Loop Until temp < 1

While Len(num) < 4
num = 0 & num
Wend

L1(I) = num

results = WorkingProg(num, I)

Next I


End Sub

Private Sub Command1_Click()
Timer1.Enabled = True

